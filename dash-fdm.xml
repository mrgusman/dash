<!--
  File:     dash-fdm.xml
  Inputs:
    name:          dash
    type:          HPA
    max weight:    275.625 lb
    wing span:     109.2573 ft
    length:        29.529 ft
    wing area:     387.151057404 sq-ft
    gear type:     tricycle
    retractable?:  no
    # engines:     1
    engine type:   piston
    engine layout: forward fuselage
    yaw damper?    no

  Outputs:
    wing loading:  0.71 lb/sq-ft
    payload:       173.8 lbs
    CL-alpha:      5.5 per radian
    CL-0:          0.25
    CL-max:        1.4
    CD-0:          0.012
    K:             0.023

-->

 <metrics>
